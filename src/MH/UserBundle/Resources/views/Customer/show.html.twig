{% extends 'MHStoreBundle:Default:layout.html.twig' %}

{% block body -%}
<div class="container">
	<div class="row">
		<div class="span12">
    <h1>Customer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ entity.firstName }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ entity.lastName }}</td>
            </tr>
            <tr>
                <th>Emailaddress</th>
                <td>{{ entity.emailAddress }}</td>
            </tr>
            <tr>
                <th>Address1</th>
                <td>{{ entity.address1 }}</td>
            </tr>
            <tr>
                <th>Address2</th>
                <td>{{ entity.address2 }}</td>
            </tr>
            <tr>
                <th>Town</th>
                <td>{{ entity.town }}</td>
            </tr>
            <tr>
                <th>County</th>
                <td>{{ entity.county }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ entity.country }}</td>
            </tr>
        </tbody>
    </table>
		</div>
	</div>
</div>
{% endblock %}
